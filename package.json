{
  "name": "deepcode",
  "//": "Changing display name requires change in general.ts",
  "displayName": "DeepCode",
  "version": "2.1.1",
  "description": "DeepCode finds bugs, security vulnerabilities, performance and API issues based on AI. DeepCode's speed of analysis allow us to analyse your code in real time and deliver results when you hit the save button in Visual Studio Code. We support Java, C/C++, JavaScript, Python, TypeScript.",
  "icon": "images/deepCodeLogo.jpg",
  "publisher": "deepcode",
  "homepage": "https://www.deepcode.ai/",
  "repository": {
    "type": "git",
    "url": "https://github.com/DeepCodeAI/vscode-extension.git"
  },
  "engines": {
    "vscode": "^1.41.0"
  },
  "galleryBanner": {
    "color": "#283e55",
    "theme": "dark"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Other"
  ],
  "keywords": [
    "Code Analysis",
    "JavaScript",
    "Java",
    "TypeScript",
    "ts",
    "Python",
    "C/C++",
    "Vue",
    "Angular",
    "React",
    "Static Code Analysis",
    "Code Quality"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": [
      {
        "title": "DeepCode",
        "properties": {
          "deepcode.url": {
            "type": "string",
            "default": "",
            "description": "In order to use a self-managed DeepCode server with Bitbucket or Gitlab instance, update the instance url below:",
            "scope": "application"
          },
          "deepcode.token": {
            "type": "string",
            "default": "",
            "description": "API key",
            "scope": "application"
          },
          "deepcode.uploadApproved": {
            "type": "boolean",
            "default": false,
            "markdownDescription": "User consent for code upload to `#deepcode.url#`",
            "scope": "window"
          },
          "deepcode.enableErrorReporter": {
            "type": "boolean",
            "default": true,
            "markdownDescription": "Enable errors to be reported to `#deepcode.url#`",
            "scope": "application"
          },
          "deepcode.enableTelemetry": {
            "type": "boolean",
            "default": true,
            "markdownDescription": "Enable usage data to be sent to `#deepcode.url#`",
            "scope": "application"
          }
        }
      }
    ],
    "commands": [
      {
        "command": "deepcode.start",
        "title": "DeepCode re-scan"
      },
      {
        "command": "deepcode.settings",
        "title": "DeepCode settings"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "@types/nock": "^11.1.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.0",
    "typescript": "^3.7.5",
    "tslint": "^5.20.1",
    "vscode-test": "^1.3.0",
    "nock": "^11.7.2"
  },
  "dependencies": {
    "@types/mz": "^0.0.32",
    "ignore": "^5.1.4",
    "mz": "^2.7.0",
    "open": "^7.0.0",
    "@deepcode/tsc": "^1.3.0"
  }
}
